{
  "name": "blnt-cli",
  "version": "0.1.0",
  "description": "AI agent that brings the power of BLNT-OS directly into your terminal with full desktop control and browser control with Autonomous Agentic Capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "blnt": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cli",
    "ai",
    "agent",
    "automation",
    "desktop-control",
    "browser-control",
    "blnt",
    "terminal"
  ],
  "author": "BLNT Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BluntmehxD/BLNT-CLI.git"
  },
  "bugs": {
    "url": "https://github.com/BluntmehxD/BLNT-CLI/issues"
  },
  "homepage": "https://github.com/BluntmehxD/BLNT-CLI#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
