{
  "name": "blnt-cli",
  "version": "0.1.0",
  "description": "AI agent that brings the power of BLNT-OS directly into your terminal with full desktop and browser control",
  "main": "dist/index.js",
  "bin": {
    "blnt": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "docs:build": "typedoc --out docs src",
    "prepublishOnly": "npm run build && npm test",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "cli",
    "ai",
    "agent",
    "automation",
    "terminal",
    "desktop-control",
    "browser-control",
    "blnt-os"
  ],
  "author": "BluntmehxD",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BluntmehxD/BLNT-CLI.git"
  },
  "bugs": {
    "url": "https://github.com/BluntmehxD/BLNT-CLI/issues"
  },
  "homepage": "https://github.com/BluntmehxD/BLNT-CLI#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6"
  }
}
