{
  "name": "blnt-cli",
  "version": "1.0.0",
  "description": "AI agent that brings the power of BLNT-OS directly into your terminal full desktop control and browser control with Autonomous Agentic Capabilities",
  "main": "dist/index.js",
  "bin": {
    "blnt": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "lint": "eslint src/",
    "format": "prettier --write src/**/*.ts",
    "test": "echo \"Tests will be added\" && exit 0",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cli",
    "automation",
    "ai-agent",
    "browser-control",
    "desktop-automation",
    "terminal",
    "autonomous-agent"
  ],
  "author": "BLNT-OS Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "inquirer": "^12.10.0",
    "node-notifier": "^10.0.1",
    "ora": "^9.0.0",
    "playwright": "^1.56.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.9.2",
    "@types/node-notifier": "^8.0.5",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.39.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  }
}
